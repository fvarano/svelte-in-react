{
	"name": "svelte-in-react",
	"version": "0.2.0",
	"description": "A lightweight library that enables using Svelte components within React applications",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"prepublishOnly": "npm run build"
	},
	"author": "Federico Varano",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/fvarano/svelte-in-react"
	},
	"keywords": [
		"react",
		"svelte",
		"adapter",
		"component",
		"integration",
		"rsc",
		"ssr",
		"svelte5"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./webpack": {
			"types": "./dist/bundlers/webpack.d.ts",
			"default": "./dist/bundlers/webpack.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"next": ">=12.0.0",
		"react": ">=17.0.0",
		"react-dom": ">=17.0.0",
		"svelte": "^5.0.0",
		"svelte-loader": ">=3.1.0"
	},
	"peerDependenciesMeta": {
		"next": {
			"optional": true
		},
		"svelte-loader": {
			"optional": true
		}
	},
	"devDependencies": {
		"@types/node": "^22",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@vitejs/plugin-react": "^4.3.4",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.3.3",
		"svelte": "5.27.2",
		"svelte-loader": "3.2.4",
		"ts-loader": "9.5.2",
		"typescript": "^5",
		"vite": "^6.0.0",
		"vite-plugin-dts": "^4.5.3"
	},
	"type": "module"
}
